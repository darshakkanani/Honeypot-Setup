# SecureHoney Docker Environment Configuration
# Copy this file to .env and update the values

# Database Configuration
POSTGRES_DB=securehoney
POSTGRES_USER=securehoney
POSTGRES_PASSWORD=securehoney123
DB_HOST=database
DB_PORT=5432

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
ADMIN_DEFAULT_PASSWORD=admin123

# Application Configuration
LOG_LEVEL=INFO
CORS_ORIGINS=http://localhost:3000,http://localhost:80

# Frontend Configuration
REACT_APP_API_URL=http://localhost:5001/api
REACT_APP_WS_URL=ws://localhost:5001/ws
REACT_APP_ENV=production

# Honeypot Configuration
HONEYPOT_SSH_PORT=2222
HONEYPOT_HTTP_PORT=8080
HONEYPOT_FTP_PORT=2121
HONEYPOT_TELNET_PORT=2323

# Monitoring (Optional)
ENABLE_MONITORING=false
LOGSTASH_HOST=logstash
LOGSTASH_PORT=5044
